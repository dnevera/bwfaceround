    <label  text="ЧТВ 8 АВГ" id="DateLabel"  x="222" y="65" justification="Gfx.TEXT_JUSTIFY_RIGHT" />

    <label  text="0" id="HourLabel0"  x="8"  y="center" font="@Fonts.ClockFont" justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label  text="0" id="HourLabel1"  x="110" y="center" font="@Fonts.ClockFont" justification="Gfx.TEXT_JUSTIFY_RIGHT" />

    <label  text=":" id="ColumnLabel" x="120" y="65" font="@Fonts.ClockFont" justification="Gfx.TEXT_JUSTIFY_CENTER" />
    <label  text="AM" id="H12Label"   x="120" y="82"  justification="Gfx.TEXT_JUSTIFY_CENTER" />

    <label  text="0" id="MinutesLabel0" x="132" y="center" font="@Fonts.ClockFont" justification="Gfx.TEXT_JUSTIFY_LEFT" />
    <label  text="0" id="MinutesLabel1" x="234" y="center" font="@Fonts.ClockFont" justification="Gfx.TEXT_JUSTIFY_RIGHT" />

    <drawable class="BMRMeter">
        <param name="width">6</param>
    </drawable>

</layout>