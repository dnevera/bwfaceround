<!-- 240x240 -->

<layout id="WatchFace">
    <drawable class="Background"></drawable>

    <drawable class="AFGraph">
        <param name="height">48</param>
        <param name="x">121</param>
        <param name="y">178</param>
    </drawable>

    <label  text="0"  id="Active0"  x="36"  y="32"  justification="Gfx.TEXT_JUSTIFY_LEFT"/>
    <label  text="0"  id="Active1"  x="109" y="32"  justification="Gfx.TEXT_JUSTIFY_CENTER"/>
    <label  text="0"  id="Active2"  x="180" y="32"  justification="Gfx.TEXT_JUSTIFY_RIGHT"/>

    <label  text="364" id="DayOfYearLabel"  x="20" y="65"  justification="Gfx.TEXT_JUSTIFY_LEFT" color="Gfx.COLOR_DK_GRAY"/>
